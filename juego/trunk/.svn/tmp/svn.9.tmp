package segundoproyectodiegoronald;

/**
 *
 * @author Diego
 */
public class Jugador {
    private String nombre;//atributo que contiene el nombre del jugador.
    private int cantVidas;//atributo que contiene la cantidad de vidas del jugador.
    private int nivel;//atributo que contiene el nivel por el cual va el jugador.
    private double puntos;//atributo que contiene la cantidad de puntos del jugador.
    private int movimientos;//atributo que contiene la cantidad de movimientos.

    public Jugador() {
        this.nombre="";
        this.cantVidas=5;
        this.nivel=0;
        this.puntos=0;
        this.movimientos=0;
    }
    

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public int getCantVidas() {
        return cantVidas;
    }

    public void setCantVidas(int cantVidas) {
        this.cantVidas = cantVidas;
    }

    public int getNivel() {
        return nivel;
    }

    public void setNivel(int nivel) {
        this.nivel = nivel;
    }

    public double getPuntos() {
        return puntos;
    }

    public void setPuntos(double puntos) {
        this.puntos = puntos;
    }

    public int getMovimientos() {
        return movimientos;
    }

    public void setMovimientos(int movimientos) {
        this.movimientos = movimientos;
    }
    
}
